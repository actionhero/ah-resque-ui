{
  "author": "Evan Tahler <evantahler@gmail.com>",
  "name": "ah-resque-ui",
  "description": "A resque administration website for actionhero",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/actionhero/ah-resque-ui.git"
  },
  "keywords": [
    "resque",
    "ui",
    "website",
    "tasks",
    "background",
    "job",
    "react"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "actionhero": ">=22.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@nivo/axes": "^0.61.0",
    "@nivo/bump": "^0.61.1",
    "@types/react": "^16.9.23",
    "actionhero": "^22.0.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "mocha": "^7.1.0",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "should": "^13.2.3",
    "standard": "^14.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "standard": {
    "globals": [
      "fetch",
      "FormData",
      "confirm",
      "before",
      "after",
      "it",
      "describe",
      "beforeEach",
      "afterEach"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server.ts",
    "start": "node ./dist/server.js",
    "pretest": "standard",
    "test": "NODE_ENV=test mocha",
    "build": "npm run ui:build && npm run api:build",
    "ui:watch": "webpack --progress --colors --watch --mode development",
    "ui:build": "webpack --mode production",
    "api:build": "rm -rf dist && ./node_modules/.bin/tsc --declaration",
    "prepublish": "npm run build"
  }
}
